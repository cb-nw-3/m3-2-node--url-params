<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/css/styles.css">
    <link rel="stylesheet" href="/css/bookStyle.css">
</head>
<body>
<header>
    <h1><%= title %></h1>
</header>
<ul style="margin: 20px;">
  <% bookGenre.forEach(genre => { %>
    <li class="books-item">
      <a href="/genre/<%= genre %>">      
        <h1 style="color: grey;"><%= genre %></h1>
      </a>

    </li>
  <% }) %>
</ul>
<div class="nav">
  <a href = "/books/">Back to Book List</a>
</div>

<%- include('../partials/footer') %>